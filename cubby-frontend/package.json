{
  "name": "cubby-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "pnpm run css:watch & wrangler dev",
    "start": "wrangler dev",
    "css:build": "./tailwindcss -i public/input.css -o public/output.css",
    "css:watch": "./tailwindcss -i public/input.css -o public/output.css --watch",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@scalar/hono-api-reference": "^0.9.22",
    "hono": "^4.9.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251004.0",
    "prettier": "^3.3.3",
    "typed-htmx": "^0.3.1",
    "typescript": "^5.0.0",
    "wrangler": "^4.42.0"
  }
}
