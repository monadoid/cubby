{
  "name": "get-cubby",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-24",
  // Bind to your R2 bucket for serving binaries
  "r2_buckets": [
    {
      "binding": "RELEASES",
      "bucket_name": "cubby-releases",
      "preview_bucket_name": "cubby-releases-preview"
    }
  ],
  // Configure bundling to include .sh files as text
  "rules": [
    {
      "type": "Text",
      "globs": ["**/*.sh"],
      "fallthrough": true
    }
  ]
}

